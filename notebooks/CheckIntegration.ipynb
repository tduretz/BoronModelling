   "source": [
    "from sympy import *\n",
    "\n",
    "t = symbols('t')\n",
    "a = Function('a')(t)\n",
    "B = Function('B')(t)\n",
    "f = 1/a * B.diff(t) - 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle -1 + \\frac{\\frac{d}{d t} B{\\left(t \\right)}}{a{\\left(t \\right)}}$"
      ],
      "text/plain": [
       "-1 + Derivative(B(t), t)/a(t)"
      ]
     },
     "execution_count": 10,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "f"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle \\int \\frac{- a{\\left(t \\right)} + \\frac{d}{d t} B{\\left(t \\right)}}{a{\\left(t \\right)}}\\, dt$"
      ],
      "text/plain": [
       "Integral((-a(t) + Derivative(B(t), t))/a(t), t)"
      ]
     },
     "execution_count": 11,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "integrate(f, t)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle - c t + B{\\left(t \\right)}$"
      ],
      "text/plain": [
       "-c*t + B(t)"
      ]
     },
     "execution_count": 17,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "c = symbols('c')\n",
    "f = B.diff(t) - c\n",
    "integrate(f, t)"
   ]
  },
